                                .Cells(row, spConDetailListIndex.ScreenNo).Text = "8" & "" & (row + 1).ToString
                        End Select

                        .Cells(row, spConDetailListIndex.StockName).Text = Trim(dr("STOCK_NAME").ToString())

                        row = row + 1
                    Next

