            Catch ex As Exception
                MsgBox(ex.ToString(), MsgBoxStyle.Exclamation)
                If UcMainStock1.EVENT_STATUS.STATUS_OnReceiveConditionVer = EventOff Then
                    UcMainStock1.OnReceiveConditionVer = EventOn
                End If
            End Try
        End If
