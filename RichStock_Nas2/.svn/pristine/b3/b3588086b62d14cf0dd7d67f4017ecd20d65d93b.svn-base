            If EVENT_STATUS.STATUS_OnReceiveTrData = EventOff Then
                EventOnOff(EventTypeIndex.OnReceiveTrData, EventOn)
            End If

            'If EVENT_STATUS.STATUS_OnReceiveRealData = EventOn Then
