                        Min_GITAGUMWOONG = CInt(.Item("기타금융합"))
                        Min_BANK = CInt(.Item("은행합"))
                        Min_YEUNGGIGUM = CInt(.Item("연기금등합"))
                        Min_SAMOFUND = CInt(.Item("사모펀드합"))
                        Min_NATION = CInt(.Item("국가합"))
