    Public Property StockCode As String
        Get
            Return _stockCode
        End Get
        Set(value As String)
            
            _stockCode = value

            lblStockCode.Text = value
            lblStockName.Text = _MainStock.GetStockInfo(value)

            _dtOpt10014.Clear()
            _dtOpt10059.Clear()
            _dtOpt10081.Clear()
            _dtOpt20068.Clear()

            lblOpt10014.Text = ""
            lblOpt10059.Text = ""
            lblOpt10081.Text = ""
            lblOpt20068.Text = ""

            If _RegisterEvent = False Then
                _RegisterEvent = True
                RegisterCallOptEvent()
                InitdtSaD()
                InitdtAutoCalSaD()
            End If

            CallOpt()

        End Set
    End Property

