☞ Child Form에서 다른 ChildForm을 띄울 때
 Private Sub ShowChildForm(ByVal childForm As System.Windows.Forms.Form)
        Dim isAlreadyContained As Boolean = False
        Dim frm As System.Windows.Forms.Form

        For Each frm In Me.MdiParent.MdiChildren
            If (frm.GetType Is childForm.GetType) Then
                isAlreadyContained = True
                frm.Activate()
                CType(frm, SgPaik.Forms.Inquiry.Order.frmOpdOrderInquiry).SetChartNo(txtChartNo.Text)
                Exit For
            End If
        Next

        If Not isAlreadyContained Then
            childForm.MdiParent = Me.MdiParent
            childForm.Show()
        Else
            childForm.Close()
        End If
    End Sub


