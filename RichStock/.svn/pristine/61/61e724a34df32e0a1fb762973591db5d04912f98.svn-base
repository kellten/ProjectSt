☞FormKeyDownHandle선언
#Region "Form의 Key Event를 제어"
    '------------------------------------------------------------
    ' 목적: Form의 Key Event를 제어한다.
    ' 입력: 없음
    ' 반환: 없음
    '------------------------------------------------------------
    Public Sub FormKeyDownHandle(ByVal sender As Object, ByVal e As System.Windows.Forms.KeyEventArgs) Handles Me.KeyDown
        If e.KeyCode &lt; Keys.F1 Or e.KeyCode &gt; Keys.F12 Then
            Exit Sub
        End If

        Windows.Forms.Cursor.Current = Windows.Forms.Cursors.WaitCursor

        Select Case e.KeyCode
            Case Keys.F5
            Case Keys.F6
                FormHandle.EraseForm(Me)

            Case Keys.F9

            Case Keys.F12
                Me.Close()
        End Select

KeyEnd:
        If e.KeyCode &lt;&gt; Keys.F12 Then
            Me.ActiveControl.Focus()
        End If
        Windows.Forms.Cursor.Current = Windows.Forms.Cursors.Default
        Exit Sub
    End Sub
#End Region


