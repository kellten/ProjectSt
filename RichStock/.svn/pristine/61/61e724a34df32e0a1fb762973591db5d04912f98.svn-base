☞DB입력코드(Transaction)
    Dim arrParam As New ArrayParam()
    Dim oSql As New SqlT("ADM", "WARDDB")

        Try
            oSql.BeginTransaction()

            With arrParam
                .Add("@QUERY", action)
                .Add("@CHART_NO", txtChartNo.Text)
            End With

            oSql.ExecuteNonQueryTransaction("StoreProcedure", CommandType.StoredProcedure, arrParam)

            oSql.CommitTransaction()

        Catch ex As Exception

            oSql.RollBack()
            MsgBox(ex.ToString, MsgBoxStyle.Exclamation)

            ClassLibrary.CFunc.MsgBar(Me.ParentForm, "작업중 Error가 발생하였습니다.")
Finally
            oSql.Endtransaction()
            Windows.Forms.Cursor.Current = Cursors.Default        
End Try



