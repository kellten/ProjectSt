' 입력: 없음
' 반환: 없음
'------------------------------------------------------------
Private Sub ExecuteReport(ByVal Destination As Integer)
    Dim arrFomulars As New ArrayParam()
    Dim arrStore As New ArrayParam()

    With arrStore
        .Add("@ACT_GB", aa)
        .Add("@FROM_DATE", aa)
        .Add("@TO_DATE", aa)
    End With

    With arrFomulars
        .Add("FROM_DATE", CDateTime.FormatDate(mskFromDate.Text, "-"))
        .Add("TO_DATE", CDateTime.FormatDate(mskToDate.Text, "-"))
        .Add("PRT_DATE", Format(Now, "yyyy-MM-dd"))
        .Add("PRT_TIME", Format(Now, "hh:mm"))
    End With

    With UcPrinterDeviceSimple
        .ExecuteReport("Ord", "Ord4320", "", "", "", _
                arrStore.ConvertArrToString, arrFomulars.ConvertArrToString, Destination, .PrintCount, "")
    End With
End Sub


